{
  "name": "tms-btf",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --base-href /amc/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "concurrently \"ng serve\" \"electron . --dev\"",
    "win-pack": "ng build --aot && electron-builder -w"
  },
  "build": {
    "appId": "org.fone.ctf",
    "productName": "amc",
    "directories": {
      "output": "win"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:5500/up/"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "dist"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "^8.2.8",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/router": "^8.2.8",
    "@ckeditor/ckeditor5-alignment": "^11.2.0",
    "@ckeditor/ckeditor5-angular": "^1.1.0",
    "@ckeditor/ckeditor5-build-classic": "^12.4.0",
    "buffer": "^5.4.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.2.1",
    "echarts": "^4.3.0",
    "maptalks": "^0.45.1",
    "moment": "^2.29.2",
    "ng-zorro-antd": "^8.3.1",
    "ngx-childprocess": "0.0.6",
    "ngx-echarts": "^4.2.1",
    "ngx-electron": "^2.1.1",
    "quill": "^1.3.7",
    "rxjs": "^6.5.3",
    "tms-platform": "^1.6.7-snapshot",
    "tms-platform-component": "^2.5.9-snapshot",
    "video.js": "^7.6.5",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "^8.3.6",
    "@angular/compiler-cli": "^8.2.8",
    "@angular/language-service": "^8.2.8",
    "@types/echarts": "^4.1.14",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.8",
    "angular-ide": "^0.9.67",
    "codelyzer": "~5.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "~3.4.0"
  }
}
